        <% for(let i in musicsVietnam) { %>
            <% if( i == 0 || i == 1 || i == 2) { %>
                <% if(i == 0) { %>
                    <li class="aside-item__list-item">
                        <div class="aside-item__list-item-image">
                            <img src="<%= musicsVietnam[i].background %>" alt="" class="aside-item__list-item-image-img">
                            <div class="aside-item__list-item-rank aside-item__list-item-rank--<%= +i+1 %>">
                                <%= +i+1 %>
                            </div>
                        </div>
                        <div class="aside-item__list-item-des">
                            <a href="/bai-hat/<%= musicsVietnam[i]._id %>" class="aside-item__list-item-des-nameMusic">
                                <%= musicsVietnam[i].name%>
                            </a>
                            <h4 class="aside-item__list-item-des-singer">
                                <% musicsVietnam[i].singers.forEach((singer, i) => { %>
                                    <% if(i != musicsVietnam[i].singers.length - 1) { %>
                                        <a href="/nghe-si/<%= singer._id %>" class="aside-item__list-item-des-singer-link"><%= singer.fullname %> </a>,
                                    <% } else {%>
                                        <a href="/nghe-si/<%= singer._id %>" class="aside-item__list-item-des-singer-link"><%= singer.fullname %> </a>
